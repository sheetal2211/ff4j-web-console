    "@angular/forms": "^7.0.1",
    "@angular/http": "^7.0.1",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "^7.0.1",
    "@angular/platform-browser-dynamic": "^7.0.1",
    "@angular/router": "^7.0.1",
    "ag-grid-angular": "^19.0.0",
    "ag-grid-community": "^19.0.0",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "ngx-logger": "^3.3.1",
    "rxjs": "^6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.3",
