{
  "name": "ff4j-web-console",
  "version": "0.0.0",
  "description": "The web console for FF4J",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paul58914080/ff4j-web-console.git"
  },
  "keywords": [
    "ff4j",
    "feature toggling",
    "console",
    "feature"
  ],
  "author": "Paul WILLIAMS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paul58914080/ff4j-web-console/issues"
  },
  "homepage": "https://paul58914080.github.io/ff4j-web-console/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock:server": "json-server --watch db.json",
    "start:proxy": "ng serve --proxy-config proxy.conf.json",
    "start:proxy:mock:server": "concurrently --kill-others \"npm run mock:server\" \"npm run start:proxy\"",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint"
  },
  "dependencies": {
    "@angular/animations": "^7.0.1",
    "@angular/cdk": "^7.0.1",
    "@angular/common": "^7.0.1",
